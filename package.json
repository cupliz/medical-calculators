{
  "name": "medical-calculators",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autosuggest-highlight": "^3.1.1",
    "axios": "^0.17.1",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.21",
    "material-ui-icons": "^1.0.0-beta.17",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-autosuggest": "^9.3.2",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.17",
    "react-slick": "^0.16.0",
    "react-swipeable-views": "^0.12.11",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "json-server": "^0.12.1",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:200": "mv build/index.html build/200.html",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "npm-run-all -p start server",
    "clean": "yarn rimraf build",
    "surge": "surge build -d medical-calc.surge.sh",
    "surge:dev": "surge build -d medical-calc-dev.surge.sh",
    "deploy": "npm-run-all -s build build:200 surge clean",
    "deploy:dev": "npm-run-all -s build build:200 surge:dev clean",
    "deploy:full": "npm-run-all -s build build:200 surge surge:dev clean",
    "server": "json-server --watch --port 3001 ./src/data/db.json",
    "now": "now --public && now alias && now remove medical-calc --safe"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "fetch"
    ]
  }
}

{
  "name": "medical-calculators",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.17.0",
    "history": "^4.7.2",
    "material-ui": "^1.0.0-beta.18",
    "material-ui-icons": "^1.0.0-beta.17",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.14",
    "react-swipeable-views": "^0.12.8",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "json-schema-faker": "^0.4.6",
    "json-server": "^0.12.0",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dev": "npm-run-all -p start server",
    "clean": "yarn rimraf build",
    "surge": "surge build -d medical-calc.surge.sh",
    "surge:dev": "surge build -d medical-calc-dev.surge.sh",
    "deploy": "npm-run-all -s build surge clean",
    "deploy:dev": "npm-run-all -s build surge:dev clean",
    "server": "json-server --watch --port 3001 ./src/data/db.json --routes routes.json",
    "now": "now ./src/data"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": ["fetch"]
  }
}
